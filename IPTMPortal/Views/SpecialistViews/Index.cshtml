@model IEnumerable<TreatmentOffering.Models.SpecialistView>

@{
    ViewData["Title"] = "Index";
}

<h1>Specialist Details</h1>


<table class="table">
    <thead>
        <tr>
            <th>
            @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Expertise)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.YearsOfExp)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Contact)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Expertise)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.YearsOfExp)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Contact)
            </td>

        </tr>
}
    </tbody>
</table>
